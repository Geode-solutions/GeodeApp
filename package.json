{
  "name": "vease",
  "version": "0.0.0-semantically-released",
  "description": "Basic app",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "npx nuxi@3.11.0 dev --no-fork",
    "generate": "nuxt generate",
    "requirements_back": "pip install pip-tools && pip-compile electron-server/requirements_back.in --upgrade",
    "requirements_viewer": "pip install pip-tools && pip-compile electron-server/requirements_viewer.in --upgrade",
    "install_back": "node node_scripts/install_back.js",
    "install_viewer": "node node_scripts/install_viewer.js",
    "build": "nuxi build --prerender",
    "test": "vitest",
    "start": "npm run build && electron-forge start",
    "prepackage": "node node_scripts/prepackage.js",
    "package": "npm run build && electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Geode-solutions/GeodeApp.git"
  },
  "keywords": [
    "Geology",
    "Visualization",
    "Web",
    "Desktop",
    "Meshing"
  ],
  "author": "Geode-solutions",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Geode-solutions/GeodeApp/issues"
  },
  "homepage": "https://github.com/Geode-solutions/GeodeApp#readme",
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/publisher-github": "^7.4.0",
    "electron": "^28.2.7",
    "nuxt": "^3.12.3",
    "nuxt-electron": "^0.7.0"
  },
  "dependencies": {
    "@fontsource/michroma": "^5.0.20",
