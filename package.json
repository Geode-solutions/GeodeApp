    "@vueuse/nuxt": "^10.9.0",
    "draggable": "^4.2.0",
    "get-port-please": "^3.1.2",
    "h3-formidable": "^1.0.0",
    "js-file-download": "^0.4.12",
    "pinia": "^2.1.7",
    "update-electron-app": "^3.0.0",
    "vue": "^3.4.21",
    "vue-cropperjs": "^5.0.0",
    "vue-router": "^4.3.0",
